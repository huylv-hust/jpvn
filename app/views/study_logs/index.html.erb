<%= javascript_include_tag "//www.google.com/jsapi", "chartkick" %>

<%= column_chart @groups_for_chart %>

<%= line_chart @groups_for_chart %>




<p id="notice"><%= notice %></p>

<h1>Listing Study Logs</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Learned word</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @study_logs.each do |study_log| %>
      <tr>
        <td><%= study_log.user_id %></td>
        <td><%= study_log.learned_word %></td>
        <td><%= link_to 'Show', study_log %></td>
        <td><%= link_to 'Edit', edit_study_log_path(study_log) %></td>
        <td><%= link_to 'Destroy', study_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Study log', new_study_log_path %>
